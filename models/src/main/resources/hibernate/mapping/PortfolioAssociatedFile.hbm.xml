<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="za.co.thecompany.mapping.PortfolioAssociatedFile" table="portfolio_associated_file"
           schema="prodo_data">
        <id name="asscociatedFileId" column="asscociated_file_id"/>
        <many-to-one name="portfolio" class="za.co.thecompany.mapping.Portfolio">
            <column name="portfolio_id"/>
        </many-to-one>
        <many-to-one name="fileInfo" class="za.co.thecompany.mapping.FileInfo">
            <column name="file_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>