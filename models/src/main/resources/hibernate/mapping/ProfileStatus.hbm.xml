<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="za.co.thecompany.mapping.ProfileStatus" table="profile_status" schema="prodo_data">
        <id name="profileStatusId" column="profile_status_id"/>
        <property name="header" column="header"/>
        <property name="profilePicturePath" column="profile_picture_path"/>
        <property name="pictureName" column="picture_name"/>
        <set name="userInfos" inverse="true">
            <key>
                <column name="profile_status_id"/>
            </key>
            <one-to-many not-found="ignore" class="za.co.thecompany.mapping.UserInfo"/>
        </set>
    </class>
</hibernate-mapping>